{
  "name": "stonks",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "dev": "rm -rf dist/* && cp ./src/main.js ./src/appsscript.json ./dist && NODE_ENV=development parcel src/index.html --dist-dir dist -p 8000",
    "start": "rm -rf dist/* && cp ./src/main.js ./src/appsscript.json ./dist && NODE_ENV=production parcel src/index.html --dist-dir dist -p 8000",
    "build": "rm -rf dist/* && cp ./src/main.js ./src/appsscript.json ./dist && parcel build src/index.html --dist-dir dist",
    "debug": "parcel src/index.html --dist-dir dist --log-level verbose -p 8000",
    "gstart": "clasp push --watch",
    "deploy": "clasp push && clasp deploy",
    "push": "npm run clean && npm run build && clasp push",
    "clean": "rm -rf .parcel-cache && rm -rf dist/*"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/material": "^5.16.0",
    "@mui/styles": "^5.16.0",
    "@mui/x-data-grid": "^7.9.0",
    "dotenv": "^16.4.5",
    "formik": "^2.4.6",
    "install": "^0.13.0",
    "npm": "^10.8.1",
    "react": "^18.3.1",
    "react-autosuggest": "^10.1.0",
    "react-dom": "^18.3.1",
    "react-loading": "^2.0.3",
    "react-router-dom": "^6.24.1",
    "sass": "^1.77.6"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "^2.12.0",
    "@parcel/transformer-svg-react": "^2.12.0",
    "@svgr/parcel-plugin-svgr": "^5.5.0",
    "@types/google-apps-script": "^1.0.83",
    "@types/node": "^20.14.10",
    "nodemon": "^3.1.4",
    "parcel": "^2.12.0",
    "process": "^0.11.10"
  }
}
